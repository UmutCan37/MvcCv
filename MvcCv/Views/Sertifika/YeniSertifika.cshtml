@model MvcCv.Models.Entity.TblSetifakalarım
@{
    ViewBag.Title = "YeniSertifika";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Sertifika Ekleme Sayfası</h4>
<br />

<form method="post">
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })
    <br />
    @Html.Label("Açıklama")
    @Html.TextBoxFor(x => x.Acıklama, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>